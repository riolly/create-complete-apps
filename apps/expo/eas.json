{
  "cli": {
    "version": ">= 3.3.0"
  },
  "build": {
    "base": {
      "node": "18.15.0",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "API_URL": "https://preview.edengarden.quest",
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "API_URL": "https://edengarden.quest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
