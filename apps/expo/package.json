{
  "name": "@acme/expo",
  "version": "0.1.1",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "expo start --dev-client",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "build:dev": "APP_VARIANT=development eas build --profile=development --platform=android",
    "build:prev": "APP_VARIANT=preview eas build --profile=preview --platform=android",
    "build:prod": "APP_VARIANT=production eas build --profile=production --platform=android",
    "build:dev-local": "APP_VARIANT=development eas build --profile=development --platform=android --local --output=\".apk/cfma-v$npm_package_version(dev)\".apk",
    "build:prev-local": "APP_VARIANT=preview eas build --profile=preview --platform=android --local --output=\".apk/cfma-v$npm_package_version(prev)\".apk",
    "build:prod-local": "APP_VARIANT=production eas build --profile=production --platform=android --local --output=\".apk/cfma-v$npm_package_version\".apk"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.18.17",
    "@expo/metro-config": "^0.10.7",
    "@shopify/flash-list": "1.5.0",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "expo": "^49.0.7",
    "expo-auth-session": "^5.0.2",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.6",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.2",
    "expo-secure-store": "^12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "^12.3.2",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.24.0",
    "superjson": "1.13.1"
  },
  "devDependencies": {
    "@acme/api": "^0.1.0",
    "@acme/eslint-config": "^0.2.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.10",
    "@expo/config-plugins": "^7.2.5",
    "@types/babel__core": "^7.20.1",
    "@types/react": "^18.2.20",
    "@types/react-native": "^0.72.2",
    "@types/webpack-env": "^1.18.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
